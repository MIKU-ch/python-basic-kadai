      "source": [
        "# 台形の各辺と高さの値を定義\n",
        "top_edge = 10  # 上辺 (cm)\n",
        "bottom_edge = 20  # 下辺 (cm)\n",
        "height = 5  # 高さ (cm)\n",
        "\n",
        "# 台形の面積を計算\n",
        "area = (top_edge + bottom_edge) * height / 2\n",
        "\n",
        "# 結果を出力\n",
        "print(f\"{area}cm²\")"
      ],
