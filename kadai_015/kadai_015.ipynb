      "source": [
        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(f\"Name: {self.name}, Age: {self.age}\")\n",
        "\n",
        "# インスタンスの作成\n",
        "person = Human(\"Alice\", 25)\n",
        "\n",
        "# 情報を出力\n",
        "person.printinfo()\n"
      ]
    }
