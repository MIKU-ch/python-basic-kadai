      "source": [
        "# リストの作成\n",
        "array = [\"水\", \"金\", \"地\", \"火\", \"木\", \"土\", \"天\", \"海\", \"冥\"]\n",
        "\n",
        "# forループを使用して出力\n",
        "print(\"=== forループの出力 ===\")\n",
        "for item in array:\n",
        "    print(item)\n",
        "\n",
        "# 空行を挿入\n",
        "print()\n",
        "\n",
        "# whileループを使用して出力\n",
        "print(\"=== whileループの出力 ===\")\n",
        "i = 0\n",
        "while i < len(array):\n",
        "    print(array[i])\n",
        "    i += 1\n"
      ],
